<Window x:Class="simpleLibrary.ReturnWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:simpleLibrary"
        mc:Ignorable="d"
        Title="ReturnWindow" Height="450" Width="713.567" Closed="Window_Closed">
    <Grid Margin="0,0,3.2,0">
        <ComboBox x:Name="cmbStock" HorizontalAlignment="Left" Margin="50,130,0,0" VerticalAlignment="Top" Width="581" Height="57"/>
        <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="50,308,0,0" VerticalAlignment="Top" Width="190" Height="52" Click="BtnCancel_Click"/>
        <Button x:Name="btnReturn" Content="Return" HorizontalAlignment="Left" Margin="441,308,0,0" VerticalAlignment="Top" Width="190" Height="52" Click="BtnReturn_Click"/>
        <Label Content="Select Stock Item to Return" HorizontalAlignment="Left" Margin="50,40,0,0" VerticalAlignment="Top" Height="68" Width="581" FontSize="36"/>

    </Grid>
</Window>
